#include<stdio.h>
int main()
{
    int a,b,c,i,k,l,temp;
    scanf("%d %d %d",&a,&b,&c);
    i=a;
    k=b;
    l=c;

    if(a>b)
    {
        temp=a;
        a=b;
        b=temp;
    }

    if(a>c)
    {
        temp=a;
        a=c;
        c=temp;
    }
    if(b>c)
    {
        temp=b;
        b=c;
        c=temp;
    }
     printf("%d\n",a);
     printf("%d\n",b);

     printf("%d\n\n",c);
     printf("%d\n",i);
     printf("%d\n",k);
     printf("%d\n",l);



    return 0;
}
